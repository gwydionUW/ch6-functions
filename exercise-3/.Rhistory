add_three <- function(input_number) {
return(input_number + 3)
}
imperial_to_metric <- function(input_feet, input_inches) {
inches <- (input_feet * 12) + input_inches
return(inches * 0.0254)
}
height_in_meters <- imperial_to_metric(6, 4)
print(height_in_meters)
